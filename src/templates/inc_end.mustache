    {{#hasblocks}}
        <i id="sidepreopen-control"
            aria-expanded="{{#draweropenright}}true{{/draweropenright}}{{^draweropenright}}false{{/draweropenright}}"
            aria-controls="sidepre-blocks"
            data-action="toggle-drawer"
            data-side="right"
            data-preference="sidepre-open"
            title="{{#str}} showhideblocks, theme_recit {{/str}}">
        </i>

        <section id="sidepre-blocks" data-region="blocks-column" class="hidden-print {{^navdraweropen}}closed{{/navdraweropen}}">
            {{{ sidepreblocks }}}
        </section>
    {{/hasblocks}}
</div><!-- Ends .page-wrapper -->

{{> theme_recit/footer }}

{{{message_drawer}}}

</body>
</html>
